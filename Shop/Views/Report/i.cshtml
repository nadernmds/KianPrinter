@model Shop.Models.User
@{
    Layout = "~/Views/Shared/_SiteLayout.cshtml";
}
<div class="container">
    <div class="form-group">
        <label class="col-sm-2 control-label" for="fromDate"> از تاریخ</label>
        <div class="col-sm-10">
            <input oninput="this.setCustomValidity('')" type="text" id="pcal5s" class="pdate" />
            <input type="hidden" id="extra" class="pdate wide" />
        </div>
    </div>
    <div class="form-group">
        <label class="col-sm-2 control-label" for="toDate">تاتاریخ</label>
        <div class="col-sm-10">
            <input oninput="this.setCustomValidity('')" type="text" id="pcal5" class="pdate" />
            <input type="hidden"  id="extra2" class="pdate wide" />
        </div>
    </div>
</div>

<script type="text/javascript">
    var objCal5 = new AMIB.persianCalendar('pcal5s', {
        extraInputID: 'extra',
        extraInputFormat: 'YYYY/MM/DD'
    }
    );
</script>
<script type="text/javascript">
    var objCal5 = new AMIB.persianCalendar('pcal5', {
        extraInputID: 'extra2',
        extraInputFormat: 'YYYY/MM/DD'
    }
    );
</script>

